<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="vmware" scheme="http://occiware.org/occi/infrastructure/vmware#" description="Mixin windows applies for image and os template on VMware compute infrastructure" specification="VMware infrastructure extension use CRTP for os tpl extended mixins">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <import href="http://schemas.ogf.org/occi/infrastructure/compute/template/1.1#/"/>
  <import href="http://schemas.ogf.org/occi/infrastructure#/"/>
  <mixins name="windows" scheme="http://occiware.org/occi/infrastructure/vmware#" title="Mixin os_tpl for Windows system">
    <attributes name="guestid" mutable="true" required="true" default="" type="//@types.0"/>
    <attributes name="datastoreisopath" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <depends href="http://schemas.ogf.org/occi/infrastructure#//@mixins[term='os_tpl']"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins name="vmaddon" scheme="http://occiware.org/occi/infrastructure/vmware#" title="Addon attributes for virtual machine (vcpu, gueststate etc.)" depends="//@mixins[term='vmimage']">
    <attributes name="vcpu" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="gueststate" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="guestosid" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="markedastemplate" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins name="vmimage" scheme="http://occiware.org/occi/infrastructure/vmware#">
    <attributes name="imagename" mutable="true" default="" description="Name of the template if this compute has a template or will be created with a template.">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins name="linux" scheme="http://occiware.org/occi/infrastructure/vmware#" title="Mixin os_tpl for linux systems">
    <attributes name="guestid" mutable="true" required="true" default="" type="//@types.0"/>
    <attributes name="datastoreisopath" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins name="macosx" scheme="http://occiware.org/occi/infrastructure/vmware#" title="Mixin os_tpl for mac os x systems">
    <attributes name="guestid" mutable="true" required="true" default="" type="//@types.0"/>
    <attributes name="datastoreisopath" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <depends href="http://schemas.ogf.org/occi/infrastructure#//@mixins[term='os_tpl']"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins name="vmwarefolders" scheme="http://occiware.org/occi/infrastructure/vmware#" title="This mixin add global attributes like datacentername, clustername, hostsystem name etc.">
    <attributes name="datacentername" mutable="true" description="Datacenter name identifier of the entity">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="datastorename" mutable="true" description="Datastore name">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="clustername" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="hostsystemname" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="inventorypath" mutable="true"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='network']"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='networkinterface']"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='storage']"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='storagelink']"/>
  </mixins>
  <mixins name="vswitchinfos" scheme="http://occiware.org/occi/infrastructure/vmware#" title="Givei informations about a vswitch">
    <attributes name="nbport" mutable="true">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='network']"/>
  </mixins>
  <mixins name="credential" scheme="http://occiware.org/occi/infrastructure/vmware#" title="Credential users information to access to ssh and userdata features">
    <attributes name="user" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="password" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="occi.compute.userdata.file" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
