<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="elasticzabbix" scheme="http://occiware.org/occi/multicloud/elasticzabbix#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <import href="http://schemas.ogf.org/occi/monitoring#/"/>
  <kinds name="ZabbixCollector" scheme="http://occiware.org/occi/multicloud/elasticzabbix#" title="Zabbix collector for computes">
    <attributes name="instanceIp" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="instanceName" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <parent href="http://schemas.ogf.org/occi/monitoring#//@kinds[term='collector']"/>
  </kinds>
  <kinds name="ZabbixSensor" scheme="http://occiware.org/occi/multicloud/elasticzabbix#">
    <parent href="http://schemas.ogf.org/occi/monitoring#//@kinds[term='sensor']"/>
  </kinds>
  <mixins name="ZabbixApiConnect" scheme="http://occiware.org/occi/multicloud/elasticzabbix#" title="Zabbix connection to the request api" depends="//@mixins[term='supervisorapiconnect']" applies="//@kinds[term='zabbixcollector']">
    <attributes name="templateName" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="hostGroupName" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="port" mutable="true">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
  </mixins>
  <mixins name="cpu" scheme="http://occiware.org/occi/multicloud/elasticzabbix#">
    <attributes name="loadAvg" mutable="true" type="//@types.0"/>
    <attributes name="cpuUsed" mutable="true" type="//@types.0"/>
    <depends href="http://schemas.ogf.org/occi/monitoring#//@mixins[term='metric']"/>
    <applies href="http://schemas.ogf.org/occi/monitoring#//@kinds[term='collector']"/>
  </mixins>
  <mixins name="SupervisorApiConnect" scheme="http://occiware.org/occi/multicloud/elasticzabbix#">
    <attributes name="username" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="password" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="httpApiAddress" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <applies href="http://schemas.ogf.org/occi/monitoring#//@kinds[term='collector']"/>
  </mixins>
  <mixins name="ram" scheme="http://occiware.org/occi/multicloud/elasticzabbix#">
    <attributes name="free" mutable="true" type="//@types.0"/>
    <attributes name="swap" mutable="true" type="//@types.0"/>
    <attributes name="ramUsed" mutable="true" type="//@types.0"/>
    <depends href="http://schemas.ogf.org/occi/monitoring#//@mixins[term='metric']"/>
    <applies href="http://schemas.ogf.org/occi/monitoring#//@kinds[term='collector']"/>
  </mixins>
  <types xsi:type="occi:NumericType" name="Percent" type="Double" minInclusive="0.0"/>
  <types xsi:type="occi:NumericType" name="PerSec" type="Double" minInclusive="0.0"/>
  <types xsi:type="occi:NumericType" name="Average" type="Double" minExclusive="" minInclusive="0.0"/>
  <types xsi:type="occi:NumericType" name="Temperature" type="Double"/>
</occi:Extension>
